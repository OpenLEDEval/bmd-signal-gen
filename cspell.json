{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "version": "0.2",
  "language": "en",
  "import": [
    "@cspell/dict-python/cspell-ext.json",
    "@cspell/dict-software-terms/cspell-ext.json",
    "@cspell/dict-cpp/cspell-ext.json",
    "@cspell/dict-data-science/cspell-ext.json"
  ],
  "words": [
    "eotf",
    "EOTF",
    "Blackmagic",
    "decklink",
    "Decklink",
    "libdecklink",
    "dylib",
    "SMPTE",
    "RAII",
    "HRESULT",
    "ctypes",
    "ndarray",
    "ascontiguousarray",
    "pyproject",
    "pyright",
    "pytest",
    "typer",
    "secho",
    "docstrings",
    "Quantizer",
    "bitdepth",
    "CCIR",
    "rgble",
    "RGBLE",
    "RGBX",
    "RGBXLE",
    "BGRA",
    "ARGB",
    "YUVA",
    "RGBW",
    "AABBGGRR",
    "AARRGGBB",
    "Gamut_Chromaticities",
    "Pyright",
    "argtypes",
    "restype",
    "meshgrid",
    "arange",
    "dataclass",
    "dtype",
    "contextlib",
    "contextmanager",
    "devnull",
    "fileno",
    "byteswap",
    "dynamiclib"
  ],
  "ignorePaths": [
    "**/node_modules/**",
    "**/.venv/**",
    "**/.git/**",
    "**/build/**",
    "**/dist/**",
    "**/__pycache__/**",
    "**/cpp/Blackmagic DeckLink SDK 14.4/**",
    "**/.memories/**",
    "**/docs/**",
    "**/sandbox/**",
    "**/*.dylib",
    "**/*.so",
    "**/*.dll"
  ],
  "ignoreRegExpList": ["\\b[A-F0-9]{8}\\b", "\\b0x[A-Fa-f0-9]+\\b"]
}
